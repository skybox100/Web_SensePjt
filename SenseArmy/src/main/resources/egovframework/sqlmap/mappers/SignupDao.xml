<mapper namespace="egovframework.SenseArmy.main.Dao.SignupDao"> <!-- resultype는 반환하는 값이 있을 경우에만 적어주는것 insert 같은 경우에는 빼준다.  -->
<insert id="signup"> <!-- 회원가입 쿼리문 -->
INSERT INTO PERSONAL_INFORMATION (ID,NAME,PHONE,MILITARY_SERIAL_NUMBER,PASSWORD) VALUES(#{param1},#{param2},#{param3},#{param4},#{param5})
</insert>
</mapper>